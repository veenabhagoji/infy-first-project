{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.roche.rgsficntrechargedefinition",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.4",
      "toolsId": "4049b389-02de-498a-a25b-ba6ee11e325f"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/odata/v4/recharge/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "catalogService": {
        "uri": "/odata/v4/recharge/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.138.1",
      "libs": {
        "sap.m": {},
        "sap.f": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.uxap": {},
        "sap.ui.comp": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.roche.rgsficntrechargedefinition.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        },
        "preload": true
      },
      "catalogService": {
        "dataSource": "catalogService",
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "updateGroupId": "actionGroup" 
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.roche.rgsficntrechargedefinition.view",
        "controlId": "flexibleColumnLayout",
        "controlAggregation": "beginColumnPages",
        "transition": "slide"
      },
      "routes": [
        {
          "pattern": "",
          "name": "master",
          "target": ["TargetRecharge"]
        },
        {
          "pattern": "detail/{rechargeTypeId}/{version}/{layout}",
          "name": "detail",
          "target": ["TargetRecharge", "TargetDetail"]
        }
      ],
      "targets": {
        "TargetRecharge": {
          "viewName": "Recharge",
          "controlAggregation": "beginColumnPages",
          "viewLevel": 1
        },
        "TargetDetail": {
          "viewName": "Detail",
          "controlAggregation": "midColumnPages",
          "viewLevel": 3
        }
      }
    },
    "rootView": {
      "viewName": "com.roche.rgsficntrechargedefinition.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
